{
    "content_security_policy": "script-src 'self' chrome://favicon/ https://www.dropbox.com/ 'unsafe-eval' 'sha256-ereB/cCTbpjzdNMeFhvyEXaG/J3ueIbnF2v70Up1U2M='; object-src 'self'",
    "manifest_version": 2,
    "name": "Initium",
    "description": "New tab page extension with additional features",
    "key": "",
    "version": "2.23.0",
    "minimum_chrome_version": "49",
    "icons": {
        "128": "assets/images/128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "geolocation",
        "topSites",
        "storage",
        "chrome://favicon/",
        "https://www.dropbox.com/"
    ],
    "web_accessible_resources": ["receiver.html"],
    "externally_connectable": {
        "matches": [
            "https://www.dropbox.com/*"
        ]
    }
}
