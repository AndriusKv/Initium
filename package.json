{
  "name": "Initium",
  "version": "0.0.0",
  "scripts": {
    "js": "webpack --progress --config ./src/webpack.config.dev.js",
    "scss": "node-sass src/scss/main.scss --source-map-embed src/css/main.css && npm run autoprefixer",
    "autoprefixer": "postcss --use autoprefixer src/css/main.css -d src/css",
    "watch:js": "webpack --progress --config ./src/webpack.config.dev.js --watch",
    "watch:css": "onchange 'src/scss/*.scss' 'src/scss/imports/*.scss' -- npm run scss",
    "watch": "npm run watch:js & npm run watch:css",
    "start": "npm run scss & npm run watch",
    "build:html": "cp ./src/index.html dist",
    "build:css": "npm run scss && csso ./src/css/main.css ./dist/css/main.css",
    "build:js": "ng2-inline /src/app/components/ && webpack --progress --config ./src/webpack.config.prod.js -p && cp -R ./src/js ./dist",
    "postbuild:js": "rm -rf ./src/app/components/ && mv ./src/app/components.old ./src/app/components",
    "build:libs": "cp -R ./src/js/libs ./dist/js",
    "build:resources": "cp -R ./src/resources ./dist",
    "build:others": "cp ./src/{sw.js,manifest.json,receiver.html,receiver.js} ./dist",
    "prebuild": "rm -rf ./dist && mkdir -p ./dist/{css,js/libs,resources}",
    "build": "npm run build:html && npm run build:css && npm run build:js && npm run build:libs && npm run build:resources && npm run build:others"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "babel-polyfill": "^6.9.1",
    "dropbox": "^2.0.0",
    "rxjs": "^5.0.0-beta.10",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "angular2-template-inline": "^1.0.6",
    "autoprefixer": "^6.3.7",
    "babel": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-angular2": "0.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.2",
    "csso": "^2.2.1",
    "eslint": "^3.1.1",
    "ncp": "^2.0.0",
    "node-sass": "^3.8.0",
    "onchange": "^2.5.0",
    "postcss-cli": "^2.5.2",
    "webpack": "^2.1.0-beta.15"
  }
}
