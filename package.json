    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.5",
    "@angular/compiler-cli": "^6.0.3",
    "@ngtools/webpack": "^6.0.5",
    "@types/chrome": "^0.0.66",
    "@types/node": "^10.1.2",
    "autoprefixer": "^8.5.1",
